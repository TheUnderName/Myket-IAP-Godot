<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ah.godot.myketinappbilling">

    <uses-permission android:name="ir.mservices.market.BILLING" />

    <queries>
     <package android:name="ir.mservices.market" />
     <intent>
         <action android:name="ir.mservices.market.InAppBillingService.BIND" />
         <data android:mimeType="*/*" />
     </intent>
    </queries>

    <application>
        <meta-data
            android:name = "org.godotengine.plugin.v1.MyketInAppBilling"
            android:value = "ah.godot.myketinappbilling.MyketInAppBillingPlugin" />
    </application>
</manifest>
